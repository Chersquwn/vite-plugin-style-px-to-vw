{
  "name": "vite-plugin-style-px-to-vw",
  "version": "0.0.0-development",
  "description": "A plugin for vite that transforms pixel units in style to viewport units.",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mts --configPlugin esbuild",
    "lint": "eslint src/*.ts",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "lint-staged": {
    "src/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "style",
    "px",
    "vw",
    "vite-plugin"
  ],
  "author": "Chersuqwn",
  "license": "MIT",
  "dependencies": {
    "change-case": "^5.2.0",
    "colors": "^1.4.0",
    "esbuild": "^0.19.7",
    "lilconfig": "^3.0.0",
    "lodash-es": "^4.17.21",
    "ts-node": "^10.9.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-alloy": "^5.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rollup": "^4.5.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "semantic-release": "^22.0.8",
    "typescript": "^5.3.2",
    "vite": "^5.0.2"
  },
  "peerDependencies": {
    "vite": "^5.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Chersquwn/vite-plugin-style-px-to-vw.git"
  }
}
